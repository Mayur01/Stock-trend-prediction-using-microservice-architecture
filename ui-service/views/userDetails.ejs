<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.4.1/css/all.css" integrity="sha384-5sAR7xN1Nv6T6+dT2mhtzEpVJvfS3NScPQTrOxhwjIuvcA67KV2R5Jz6kr4abQsz" crossorigin="anonymous">
    <link href="https://fonts.googleapis.com/css?family=Nunito:400,600|Open+Sans:400,600,700" rel="stylesheet">
    <link rel="stylesheet" href="../css/spur.css">
    <script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
    <title>Stock Prediction using Microservices</title>
</head>

<body>
    <div class="dash">
        <div class="dash-nav dash-nav-dark">
            <header>
                <a href="#!" class="menu-toggle">
                    <i class="fas fa-bars"></i>
                </a>
                <!-- <a href="#" class="spur-logo"><span>User Details</span></a> -->
                <a href="#" class="spur-logo"><i class="fas fa-bolt"></i> <span>User Details</span></a>
            </header>
            <nav class="dash-nav-list">
                <a href="index" class="dash-nav-item">
                    <i class="fas fa-home"></i> Dashboard </a>
                <div class="dash-nav-dropdown">
                    <a href="#!" class="dash-nav-item dash-nav-dropdown-toggle">
                        <i class="fab fa-bitcoin"></i> Stocks Owned </a>
                    <div id="stocksOwned" class="dash-nav-dropdown-menu">
                        <% for (const [key, value] of Object.entries(results.Stocks)) { %>
                            <a href="./stocks?Firstname=<%=firstname%>&stockname=<%=key%>" class="dash-nav-dropdown-item"><%=key%></a>
                        <% } %>
                    </div>
                </div>
                <div class="dash-nav-dropdown">
                    <a href="#!" class="dash-nav-item dash-nav-dropdown-toggle">
                        <i class="fas fa-info"></i> Team 07 </a>
                    <div class="dash-nav-dropdown-menu">
                        <a href="https://chaitanya1731.github.io/" target="_blank" class="dash-nav-dropdown-item">Chaitanya K</a>
                        <a href="" target="_blank" class="dash-nav-dropdown-item">Prathamesh W</a>
                        <a href="" target="_blank" class="dash-nav-dropdown-item">Mayur K</a>
                    </div>
                </div>
            </nav>
        </div>
        <div class="dash-app">
            <header class="dash-toolbar">    
                <a href="#!" class="menu-toggle">
                    <i class="fas fa-bars"></i>
                </a>
                <div class="container text-center">
                  <h1>Stock Trend Prediction using Microservices</h1>
                </div>
            </header>
            <main class="dash-content">
                <div class="container-fluid">
                    <div class="row dash-row">
                        <div class="col-xl-12">
                            <div class="stats stats-dark">
                                <h2 id="username" class="stats-title"><%=results.Name%></h2>
                                <div class="stats-content">
                                    <div class="stats-icon">
                                        <i class="fas fa-user"></i>
                                    </div>
                                    <div class="stats-data">
                                        <div id="totalInvestedAmount" class="stats-number"></div>
                                        <!-- <div class="stats-change">
                                            <span class="stats-percentage">+25%</span>
                                            <span class="stats-timeframe">from last month</span>
                                        </div> -->
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xl-12">
                            <div class="card spur-card">
                                <div class="card-header">
                                    <div class="spur-card-icon">
                                        <i class="fab fa-bitcoin"></i>
                                    </div>
                                    <div class="spur-card-title">Stocks Owned </div>
                                </div>
                                <div class="card-body ">
                                    <table id="stocksTable" class="table table-hover table-in-card">
                                        <thead>
                                            <tr>
                                                <th scope="col">#</th>
                                                <th scope="col">Stock Name</th>
                                                <th scope="col"># of Shares</th>
                                                <th scope="col">Per Share Amount ($)</th>
                                                <th scope="col">Total Invested Amount ($)</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                        <%
                                        let i=1;
                                        for (const [key, value] of Object.entries(results.Stocks)) { %>
                                        <tr>
                                            <th scope="row"><%=i%></th>
                                            <td><a href="./stocks?Firstname=<%=firstname%>&stockname=<%=key%>"><%=key%></a></td>
                                            <td><%=value.shares %></td>
                                            <td><%=value.price %>$</td>
                                            <td><%=(value.shares*value.price).toFixed(2)%>$</td>
                                        </tr>
                                        <% i++; } %>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </main>
        </div>
    </div>
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
    <script src="../js/spur.js"></script>
    <script type="text/javascript" src="https://canvasjs.com/assets/script/jquery-1.11.1.min.js"></script>
    <script src="js/userDetails-js.js"></script>
</body>

</html>